{
	"name": "angular-lab",
	"version": "0.0.0",
	"description": "Angular Laboratory",
	"keywords": [
		"angular 2",
		"angular 2 seed",
		"angular 2 demo app",
		"angular 2 example",
		"angular"
	],
	"homepage": "https://ng2-lab.firebaseapp.com",
	"license": "MIT",
	"author": {
		"name": "Roland Groza",
		"email": "rolandjitsu@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rolandjitsu/ng2-lab.git"
	},
	"scripts": {
		"ng": "ng",
		"start": "ng serve --open --hmr",
		"start:staging": "ng serve --open --hmr --environment=staging",
		"start:prod": "ng serve --open --hmr --environment=prod",
		"build": "ng build --sourcemaps --output-hashing=bundles",
		"build:staging": "ng build --target=production --environment=staging --sourcemaps --output-hashing=bundles --no-progress",
		"build:prod": "ng build --target=production --environment=prod --output-hashing=bundles --no-progress",
		"lint": "ng lint",
		"test": "ng test",
		"test:ci": "ng test --no-progress --browsers PhantomJS --single-run",
		"wd:update": "webdriver-manager update --standalone false --gecko false",
		"e2e": "npm run wd:update && ng e2e",
		"e2e:ci": "npm run wd:update && ng e2e --no-progress"
	},
	"dependencies": {
		"@angular/common": "^2.4.7",
		"@angular/compiler": "^2.4.7",
		"@angular/core": "^2.4.7",
		"@angular/forms": "^2.4.7",
		"@angular/http": "^2.4.7",
		"@angular/platform-browser": "^2.4.7",
		"@angular/platform-browser-dynamic": "^2.4.7",
		"@angular/router": "^3.4.7",
		"@angular/material": "latest",
		"core-js": "^2.4.1",
		"hammerjs": "^2.0.8",
		"reflect-metadata": "^0.1.9",
		"rxjs": "^5.0.1",
		"zone.js": "0.7.7"
	},
	"devDependencies": {
		"@types/hammerjs": "^2.0.33",
		"@types/jasmine": "^2.5.38",
		"@types/jasmine-expect": "^2.0.29",
		"@types/node": "^6.0.48",
		"@angular/cli": "latest",
		"@angular/compiler-cli": "^2.4.7",
		"@angular/tsc-wrapped": "latest",
		"codelyzer": "^2.0.0-beta.4",
		"jasmine": "^2.5.2",
		"jasmine-core": "^2.5.2",
		"jasmine-expect": "^3.0.1",
		"jasmine-spec-reporter": "^2.7.0",
		"karma": "^1.3.0",
		"karma-chrome-launcher": "^2.0.0",
		"karma-cli": "^1.0.1",
		"karma-jasmine": "^1.0.2",
		"karma-phantomjs-launcher": "^1.0.2",
		"karma-coverage-istanbul-reporter": "^0.2.0",
		"phantomjs-prebuilt": "^2.1.13",
		"protractor": "^4.0.13",
		"protractor-accessibility-plugin": "^0.2.0",
		"protractor-console-plugin": "^0.1.1",
		"tslint": "^4.4.2",
		"ts-helpers": "^1.1.2",
		"ts-node": "^1.7.0",
		"typescript": "2.0.10",
		"webdriver-manager": "^12.0.2",
		"yargs": "^6.6.0"
	},
	"engines": {
		"node": ">= 7.4"
	}
}
