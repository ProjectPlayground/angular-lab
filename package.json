{
	"name": "ng2-lab",
	"version": "0.0.0",
	"description": "Angular 2 Laboratory",
	"keywords": [
		"angular 2",
		"angular 2 experiments",
		"angular",
		"angular 2 demo app",
		"angular 2 example"
	],
	"homepage": "https://ng2-lab.firebaseapp.com",
	"license": "MIT",
	"author": {
		"name": "Roland Groza",
		"email": "rolandjitsu@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rolandjitsu/ng2-lab"
	},
	"scripts": {
		"build": "gulp clean && gulp build",
		"test": "gulp lint && gulp test/unit:single && gulp test/e2e:local",
		"deploy": "gulp deploy",
		"start": "gulp serve"
	},
	"dependencies": {},
	"devDependencies": {
		"babel-core": "6.7.x",
		"babel-preset-es2015": "6.6.x",
		"babel-preset-stage-1": "6.5.x",
		"browser-sync": "2.11.x",
		"del": "2.2.x",
		"firebase-tools": "2.2.x",
		"gulp": "gulpjs/gulp#4.0",
		"gulp-autoprefixer": "3.1.x",
		"gulp-changed": "1.3.x",
		"gulp-plumber": "1.1.x",
		"gulp-size": "2.1.x",
		"gulp-sourcemaps": "1.6.x",
		"gulp-tslint": "4.3.x",
		"gulp-typescript": "2.12.x",
		"gulp-util": "3.0.x",
		"jasmine": "2.4.x",
		"jasmine-core": "2.4.x",
		"jasmine-spec-reporter": "2.4.x",
		"jspm": "beta",
		"karma": "0.13.x",
		"karma-chrome-launcher": "0.2.x",
		"karma-cli": "0.1.x",
		"karma-jasmine": "0.3.x",
		"karma-jspm": "2.1.x",
		"karma-sauce-launcher": "0.3.x",
		"karma-spec-reporter": "0.0.x",
		"open": "0.0.x",
		"protractor": "3.2.x",
		"protractor-accessibility-plugin": "0.1.x",
		"protractor-console-plugin": "0.1.x",
		"sauce-connect-launcher": "0.14.x",
		"split2": "2.0.x",
		"through2": "2.0.x",
		"tslint": "next",
		"typescript": "next",
		"typings": "0.7.x",
		"yargs": "4.3.x"
	},
	"engines": {
		"node": "5.10.x"
	},
	"private": true,
	"jspm": {
		"name": "ng2-lab",
		"dependencies": {
			"angular2": "npm:angular2@^2.0.0-beta.13",
			"firebase": "github:firebase/firebase-bower@^2.4.1",
			"reflect-metadata": "npm:reflect-metadata@0.1.2",
			"rxjs": "npm:rxjs@5.0.0-beta.2",
			"zone.js": "npm:zone.js@^0.6.5"
		},
		"devDependencies": {
			"babel": "npm:systemjs-plugin-babel@^0.0.8",
			"ts": "github:frankwallis/plugin-typescript@^4.0.2"
		},
		"peerDependencies": {
			"assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
			"buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
			"child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
			"constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
			"crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
			"es6-promise": "npm:es6-promise@^3.0.2",
			"es6-shim": "npm:es6-shim@^0.35.0",
			"events": "github:jspm/nodelibs-events@^0.2.0-alpha",
			"fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
			"path": "github:jspm/nodelibs-path@^0.2.0-alpha",
			"process": "github:jspm/nodelibs-process@^0.2.0-alpha",
			"stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
			"string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
			"util": "github:jspm/nodelibs-util@^0.2.0-alpha",
			"vm": "github:jspm/nodelibs-vm@^0.2.0-alpha"
		},
		"overrides": {
			"github:firebase/firebase-bower@2.4.1": {
				"main": "firebase",
				"registry": "jspm"
			},
			"npm:angular2@2.0.0-beta.13": {
				"main": false,
				"dependencies": {
					"reflect-metadata": "npm:reflect-metadata@0.1.2",
					"rxjs": "npm:rxjs@^5.0.0-beta.2",
					"zone.js": "npm:zone.js@^0.6.6"
				},
				"peerDependencies": {
					"reflect-metadata": "npm:reflect-metadata@0.1.2",
					"rxjs": "npm:rxjs@^5.0.0-beta.2",
					"zone.js": "npm:zone.js@^0.6.6"
				},
				"jspmNodeConversion": false,
				"map": {
					"crypto": "@empty"
				},
				"meta": {
					"src/*.js": {
						"deps": [
							"reflect-metadata",
							"zone.js"
						]
					}
				}
			},
			"npm:inherits@2.0.1": {
				"ignore": [
					"test.js"
				]
			},
			"npm:reflect-metadata@0.1.2": {
				"map": {
					"crypto": "@empty"
				}
			},
			"npm:typescript@1.8.9": {
				"browser": {},
				"map": {
					"buffer": "@empty",
					"child_process": "@empty",
					"fs": "@empty",
					"os": "@empty",
					"path": "@empty",
					"process": "@empty",
					"readline": "@empty"
				}
			}
		}
	}
}
